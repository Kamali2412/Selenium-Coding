package com.allProgram;

import java.util.Scanner;

public class Reverse_Numbers {
	
	public static void main(String[] args) {
		
	Scanner Sc = new Scanner(System.in);
	
	int num = Sc.nextInt();
	
	int rev = 0;
	
	while (num!=0);
	
	{
		rev = rev/10 + num%10;
		
		num = num/10;
	}
		
	System.out.println(num);
	}

}
