package com.allProgram;

import java.util.Scanner;

public class Reverse_String {
	
	public static void main(String[] args) {
		
		
		String str = "PQRST";
		String rev = "";
		
		int len = str.length();
		 
		for (int i=len-1; len>=0; i--)
			
		{
			rev = rev + str.charAt(i);
		}
			
		System.out.println(rev);
		
	}

}
